{
  "name": "PlaidTab",
  "manifest_version": 2,
  "version": "0.0.0.1",
  "description": "A Chrome extension new tab dashboard created for CMU students.",
  "browser_action": {
  	"default_icon": "img/icon.jpg"
  },
  "chrome_url_overrides": {
    "newtab": "log_in.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}